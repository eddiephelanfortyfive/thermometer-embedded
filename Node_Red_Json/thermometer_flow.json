    {
        "id": "60a9108ddf33a56f",
        "type": "function",
        "z": "1ba1eefc79b1c65a",
        "name": "Status colour",
        "func": "// msg.payload = { status, uptime_ms, buffered, state, reasons: [...] }\n\nconst p = msg.payload || {};\nconst rawState = (p.state || \"OK\").toUpperCase();   // \"CRITICAL\" here\nconst reasons = Array.isArray(p.reasons) ? p.reasons : [];\nconst reason = reasons[0] || \"\";\n\n// Map state to label + colour\nlet label = \"OK\";\nlet color = \"green\";\n\nif (rawState === \"WARN\" || rawState === \"WARNING\") {\n  label = \"WARNING\";\n  color = \"orange\";\n} else if (rawState === \"CRITICAL\") {\n  label = \"CRITICAL\";\n  color = \"red\";\n}\n\nmsg.payload.stateLabel = label;\nmsg.payload.stateColor = color;\nmsg.payload.stateReason = reason;\n\nreturn msg;\n",
